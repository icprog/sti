## Process this file with automake to produce GNUmakefile.in
SUBDIRS = corba python

AM_CPPFLAGS = -Wall -Werror $(ORB_CXXFLAGS) \
    -I$(top_srcdir)/../device/include -Ipython
AM_LDFLAGS  = $(ORB_LDFLAGS)
LDADD       = $(ORB_LIBS)

bin_PROGRAMS = server
server_SOURCES = server.cpp
server_LDADD = \
    $(ORB_LIBS) \
    corba/libcorba.a \
    python/libpython.a
